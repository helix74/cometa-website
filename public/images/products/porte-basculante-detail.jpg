<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-basculante" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F7FAFC" />
      <stop offset="100%" style="stop-color:#EDF2F7" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-basculante)"/>
  
  <!-- Structure garage -->
  <rect x="50" y="80" width="300" height="170" fill="none" stroke="#1A202C" stroke-width="2"/>
  
  <!-- Porte basculante (panel unique) -->
  <rect x="60" y="90" width="280" height="150" fill="#4A5568" stroke="#F6AD55" stroke-width="2" rx="5"/>
  
  <!-- Nervures de renfort -->
  <line x1="80" y1="110" x2="320" y2="110" stroke="#F6AD55" stroke-width="1"/>
  <line x1="80" y1="140" x2="320" y2="140" stroke="#F6AD55" stroke-width="1"/>
  <line x1="80" y1="170" x2="320" y2="170" stroke="#F6AD55" stroke-width="1"/>
  <line x1="80" y1="200" x2="320" y2="200" stroke="#F6AD55" stroke-width="1"/>
  <line x1="80" y1="220" x2="320" y2="220" stroke="#F6AD55" stroke-width="1"/>
  
  <!-- Nervures verticales -->
  <line x1="130" y1="95" x2="130" y2="235" stroke="#F6AD55" stroke-width="1"/>
  <line x1="200" y1="95" x2="200" y2="235" stroke="#F6AD55" stroke-width="1"/>
  <line x1="270" y1="95" x2="270" y2="235" stroke="#F6AD55" stroke-width="1"/>
  
  <!-- Ressorts d'équilibrage (visibles) -->
  <path d="M55 100 Q60 95 65 100 Q70 105 75 100 Q80 95 85 100" stroke="#3182CE" stroke-width="3" fill="none"/>
  <path d="M315 100 Q320 95 325 100 Q330 105 335 100 Q340 95 345 100" stroke="#3182CE" stroke-width="3" fill="none"/>
  
  <!-- Axes de basculement -->
  <circle cx="60" cy="165" r="4" fill="#F6AD55"/>
  <circle cx="340" cy="165" r="4" fill="#F6AD55"/>
  
  <!-- Poignée centrale -->
  <rect x="190" y="155" width="20" height="10" fill="#F6AD55" rx="5"/>
  
  <!-- Serrure simple -->
  <rect x="195" y="170" width="10" height="15" fill="#3182CE" rx="2"/>
  
  <!-- Indicateur économique -->
  <rect x="320" y="100" width="40" height="15" fill="#10B981" rx="3"/>
  <text x="340" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">ECO</text>
  
  <!-- Titre produit -->
  <text x="200" y="270" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1A202C">
    Porte Basculante
  </text>
  <text x="200" y="290" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#4A5568">
    Solution Économique
  </text>
</svg> 