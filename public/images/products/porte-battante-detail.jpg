<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-battante" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F7FAFC" />
      <stop offset="100%" style="stop-color:#EDF2F7" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-battante)"/>
  
  <!-- Structure garage -->
  <rect x="50" y="80" width="300" height="170" fill="none" stroke="#1A202C" stroke-width="2"/>
  
  <!-- Porte battante gauche (ouverte) -->
  <rect x="50" y="90" width="120" height="150" fill="#3182CE" stroke="#F6AD55" stroke-width="2" rx="5" transform="rotate(-30 110 165)"/>
  
  <!-- Porte battante droite (fermée) -->
  <rect x="230" y="90" width="120" height="150" fill="#4A5568" stroke="#F6AD55" stroke-width="2" rx="5"/>
  
  <!-- Panneaux décoratifs porte droite -->
  <rect x="240" y="100" width="100" height="40" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1" rx="3"/>
  <rect x="240" y="150" width="100" height="40" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1" rx="3"/>
  <rect x="240" y="200" width="100" height="30" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1" rx="3"/>
  
  <!-- Panneaux décoratifs porte gauche (transformée) -->
  <rect x="60" y="100" width="100" height="40" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1" rx="3" transform="rotate(-30 110 120)"/>
  <rect x="60" y="150" width="100" height="40" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1" rx="3" transform="rotate(-30 110 170)"/>
  
  <!-- Charnières -->
  <circle cx="50" cy="110" r="3" fill="#4A5568"/>
  <circle cx="50" cy="140" r="3" fill="#4A5568"/>
  <circle cx="50" cy="170" r="3" fill="#4A5568"/>
  <circle cx="50" cy="200" r="3" fill="#4A5568"/>
  
  <circle cx="230" cy="110" r="3" fill="#4A5568"/>
  <circle cx="230" cy="140" r="3" fill="#4A5568"/>
  <circle cx="230" cy="170" r="3" fill="#4A5568"/>
  <circle cx="230" cy="200" r="3" fill="#4A5568"/>
  
  <!-- Poignées -->
  <rect x="320" y="160" width="20" height="8" fill="#F6AD55" rx="4"/>
  <rect x="140" y="160" width="20" height="8" fill="#F6AD55" rx="4" transform="rotate(-30 150 164)"/>
  
  <!-- Serrures centrales -->
  <rect x="345" y="155" width="8" height="20" fill="#3182CE" rx="2"/>
  <rect x="170" y="155" width="8" height="20" fill="#3182CE" rx="2"/>
  
  <!-- Indicateur style traditionnel -->
  <rect x="290" y="100" width="50" height="15" fill="#8B4513" rx="3"/>
  <text x="315" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="white">STYLE</text>
  
  <!-- Titre produit -->
  <text x="200" y="270" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1A202C">
    Porte Battante
  </text>
  <text x="200" y="290" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#4A5568">
    Style Traditionnel
  </text>
</svg> 