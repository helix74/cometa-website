<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-enroul-detail" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F7FAFC" />
      <stop offset="100%" style="stop-color:#EDF2F7" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-enroul-detail)"/>
  
  <!-- Structure du garage -->
  <rect x="60" y="80" width="280" height="160" fill="none" stroke="#1A202C" stroke-width="2"/>
  
  <!-- Caisson d'enroulement compact -->
  <rect x="70" y="70" width="260" height="25" fill="#3182CE" rx="12"/>
  <circle cx="200" cy="82" r="8" fill="#F6AD55"/>
  <text x="200" y="86" text-anchor="middle" font-family="Arial, sans-serif" font-size="6" font-weight="bold" fill="#1A202C">M</text>
  
  <!-- Rideau fermé -->
  <rect x="80" y="95" width="240" height="140" fill="#1A202C"/>
  
  <!-- Lames du rideau avec sécurité -->
  <line x1="80" y1="105" x2="320" y2="105" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="120" x2="320" y2="120" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="135" x2="320" y2="135" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="150" x2="320" y2="150" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="165" x2="320" y2="165" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="180" x2="320" y2="180" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="195" x2="320" y2="195" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="210" x2="320" y2="210" stroke="#F6AD55" stroke-width="2"/>
  <line x1="80" y1="225" x2="320" y2="225" stroke="#F6AD55" stroke-width="2"/>
  
  <!-- Coulisses de sécurité -->
  <rect x="75" y="95" width="8" height="140" fill="#4A5568"/>
  <rect x="320" y="95" width="8" height="140" fill="#4A5568"/>
  
  <!-- Serrures anti-effraction -->
  <rect x="150" y="230" width="15" height="8" fill="#F6AD55" rx="2"/>
  <rect x="235" y="230" width="15" height="8" fill="#F6AD55" rx="2"/>
  
  <!-- Indicateurs gain de place -->
  <path d="M20 150 L50 120" stroke="#3182CE" stroke-width="2"/>
  <path d="M45 125 L50 120 L45 130" stroke="#3182CE" stroke-width="2" fill="none"/>
  <text x="15" y="170" font-family="Arial, sans-serif" font-size="10" fill="#3182CE">Compact</text>
  
  <!-- Télécommande -->
  <rect x="340" y="200" width="40" height="25" fill="#3182CE" rx="5"/>
  <circle cx="355" cy="210" r="2" fill="#F6AD55"/>
  <circle cx="365" cy="210" r="2" fill="#F6AD55"/>
  
  <!-- Titre produit -->
  <text x="200" y="270" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1A202C">
    Porte à Enroulement
  </text>
  <text x="200" y="290" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#4A5568">
    Gain de Place Maximal
  </text>
</svg> 