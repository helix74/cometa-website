<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-sectionnelle" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#F7FAFC" />
      <stop offset="100%" style="stop-color:#EDF2F7" />
    </linearGradient>
  </defs>
  <rect width="400" height="300" fill="url(#bg-sectionnelle)"/>
  
  <!-- Structure garage -->
  <rect x="50" y="80" width="300" height="170" fill="none" stroke="#1A202C" stroke-width="2"/>
  
  <!-- Porte sectionnelle avec panneaux isolants -->
  <rect x="60" y="90" width="280" height="150" fill="#3182CE" stroke="#F6AD55" stroke-width="2" rx="5"/>
  
  <!-- Panneaux sectionnels avec isolation visible -->
  <rect x="70" y="100" width="260" height="30" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1"/>
  <rect x="70" y="135" width="260" height="30" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1"/>
  <rect x="70" y="170" width="260" height="30" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1"/>
  <rect x="70" y="205" width="260" height="30" fill="#E2E8F0" stroke="#F6AD55" stroke-width="1"/>
  
  <!-- Rails de guidage verticaux -->
  <rect x="55" y="90" width="8" height="150" fill="#4A5568"/>
  <rect x="340" y="90" width="8" height="150" fill="#4A5568"/>
  
  <!-- Rails horizontaux -->
  <rect x="50" y="75" width="300" height="8" fill="#4A5568"/>
  
  <!-- Isolation thermique (points) -->
  <circle cx="100" cy="115" r="2" fill="#F6AD55"/>
  <circle cx="120" cy="115" r="2" fill="#F6AD55"/>
  <circle cx="140" cy="115" r="2" fill="#F6AD55"/>
  <circle cx="160" cy="115" r="2" fill="#F6AD55"/>
  
  <!-- Ressorts d'équilibrage -->
  <path d="M55 85 Q60 80 65 85 Q70 90 75 85" stroke="#3182CE" stroke-width="2" fill="none"/>
  <path d="M325 85 Q330 80 335 85 Q340 90 345 85" stroke="#3182CE" stroke-width="2" fill="none"/>
  
  <!-- Motorisation -->
  <rect x="360" y="100" width="30" height="20" fill="#F6AD55" rx="3"/>
  <text x="375" y="113" text-anchor="middle" font-family="Arial, sans-serif" font-size="8" font-weight="bold" fill="#1A202C">M</text>
  
  <!-- Poignée ergonomique -->
  <rect x="310" y="155" width="20" height="8" fill="#F6AD55" rx="4"/>
  
  <!-- Titre produit -->
  <text x="200" y="270" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#1A202C">
    Porte Sectionnelle
  </text>
  <text x="200" y="290" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#4A5568">
    Isolation Optimale
  </text>
</svg> 